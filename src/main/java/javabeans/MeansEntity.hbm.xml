<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="javabeans.MeansEntity" table="means" schema="db_dictionary">
        <property name="means">
            <column name="means" sql-type="varchar(1000)" length="1000" not-null="true"/>
        </property>
        <many-to-one name="wordsByWordId" class="javabeans.WordsEntity">
            <column name="wordID"/>
        </many-to-one>
        <many-to-one name="posByPosId" class="javabeans.PosEntity">
            <column name="posID"/>
        </many-to-one>
    </class>
</hibernate-mapping>