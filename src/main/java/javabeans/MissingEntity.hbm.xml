<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="javabeans.MissingEntity" table="missing" schema="db_dictionary">
        <id name="id">
            <column name="ID" sql-type="int(20)"/>
        </id>
        <property name="word">
            <column name="word" sql-type="varchar(200)" length="200"/>
        </property>
    </class>
</hibernate-mapping>