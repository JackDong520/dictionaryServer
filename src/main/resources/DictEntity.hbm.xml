<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="javabeans.DictEntity" table="dict" schema="dic_server">
        <id name="id"/>
        <property name="word">
            <column name="word" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="meaning">
            <column name="meaning" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>